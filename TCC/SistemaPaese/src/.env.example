# === Configurações do Banco de Dados ===
DB_HOST=caminho para o banco
DB_PORT=numero da porta
DB_NAME=nome do banco
DB_USERNAME=seu_usuario
DB_PASSWORD=sua_senha

# === Configurações de Segurança (JWT, etc.) ===
JWT_SECRET=sua_chave_secreta_muito_forte
JWT_EXPIRATION_MS=3600000        # 1 hora em milissegundos
JWT_REFRESH_EXPIRATION_MS=604800000  # 7 dias

# === Configurações da Aplicação ===
SERVER_PORT=numero da porta do servidor
SPRING_PROFILES_ACTIVE=dev

# === E-mail (se for usar envio de e-mails com Outlook, por exemplo) ===
MAIL_HOST=smtp.office365.com
MAIL_PORT=587
MAIL_USERNAME=seu_email@outlook.com
MAIL_PASSWORD=sua_senha
MAIL_FROM=seu_email@outlook.com
MAIL_FROM_NAME=Sistema Paese
